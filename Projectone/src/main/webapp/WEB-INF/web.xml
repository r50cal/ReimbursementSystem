<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>DispatcherServlet</servlet-name>
  	<display-name>DispatcherServlet</display-name>
  	<description></description>
  	<servlet-class>dev.calapatia.servlets.DispatcherServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>dev.calapatia.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeReimbursementInfo</servlet-name>
  	<display-name>EmployeeReimbursementInfo</display-name>
  	<description></description>
  	<servlet-class>dev.calapatia.servlets.EmployeeReimbursementInfo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ManagerLoginServlet</servlet-name>
  	<display-name>ManagerLoginServlet</display-name>
  	<description></description>
  	<servlet-class>dev.calapatia.servlets.ManagerLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>DispatcherServlet</servlet-name>
  	<url-pattern>/api/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeReimbursementInfo</servlet-name>
  	<url-pattern>/EmployeeReimbursementInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ManagerLoginServlet</servlet-name>
  	<url-pattern>/ManagerLoginServlet</url-pattern>
  </servlet-mapping>
</web-app>
